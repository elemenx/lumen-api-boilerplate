{
    "name": "elemenx/api-starter",
    "description": "API Boilerplate Based on Lumen",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.x-dev@dev",
        "vlucas/phpdotenv": "~2.2",
        "tymon/jwt-auth": "1.0.*@RC",
        "league/flysystem": "^1.0",
        "intervention/image": "^2.4",
        "nordsoftware/lumen-cors": "^2.2",
        "illuminate/redis": "^5.6",
        "overtrue/easy-sms": "^0.0.10",
        "laravelista/lumen-vendor-publish": "^2.1",
        "sentry/sentry-laravel": "^0.8.0",
        "elemenx/lumen-advanced-route": "^0.1.4",
        "elemenx/laravel-api-pagination": "^0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
